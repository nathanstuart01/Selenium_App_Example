<h1>Beers</h1>
<div class='center'>
  <img src='data:image/jpeg;base64,' />
  <br />
<%= link_to 'Grab A Beer!', beers_path, class: 'btn', method: :post %>
</div>


<div class='center'>
  <% if @non_empty.length <= 10 %>
      <%= @non_empty.length > 10 ? 'You are cut off!' : 'Keep drinking' %>
    <% else %>
      <h3 class='red-text'>'You are cut off'</h3>
  <% end %>
</div>

<table>
  <thead>
    <tr>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @beers.each do |beer| %>
      <tr>
        <td><%= beer.name %></td>
        <td><%= link_to 'Show', beer %></td>
        <td><%= link_to 'Edit', edit_beer_path(beer) %></td>
        <td><%= link_to 'Destroy', beer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
